#ifndef UNLIMITED_CODE_WORKS_STD
#define UNLIMITED_CODE_WORKS_STD

#if defined(__APPLE__) || defined(__APPLE_CC__) || defined(__OSX__)
#include <cul/stdc++.h>
#else
#include <bits/stdc++.h>
#endif

using namespace std;

typedef int32_t i32;
typedef int64_t i64;
typedef uint32_t u32;
typedef uint64_t u64;

typedef long double ld;

typedef std::pair<i32, i32> pii;
typedef std::vector<i32> vi;
typedef std::vector<vi> vvi;
typedef std::vector<pii> vpii;

#define rep(i, a, b) for(auto i=a, i##_n=b; i<i##_n; ++i)
#define per(i, a, b) for(auto i=b, i##_n=a; i-->i##_n; )

#endif //UNLIMITED_CODE_WORKS_STD
